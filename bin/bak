#!/usr/bin/perl
use warnings;use strict;use utf8;use v5.22;use Oct::d8;my $d8nw=Oct::d8->new();
open my $out8,'>&',STDOUT or die "Can't open  duplic8 STDOUT handle: $!";binmode $out8,':encoding(UTF-8)';print $out8 $d8nw->bak(@ARGV);
close   $out8             or die "Can't close duplic8 STDOUT handle: $!";
